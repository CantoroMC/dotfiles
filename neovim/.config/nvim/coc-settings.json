{
	"coc.preferences.useQuickfixForLocations": true,
	"coc.preferences.hoverTarget": "float",
	"coc.source.syntax.priority": 99,
	"coc.source.syntax.filetypes": ["arch", "awk", "automake", "cabal", "cmake", "conf", "config", "conkyrc", "crontab", "desktop", "diff", "dircolors", "dirpager", "fstab", "git", "gitcommit", "gitconfig", "gitsendmail", "godoc", "gpg", "groff", "help", "html", "i3", "less", "mail", "mailaliases", "make", "man", "manconf", "manual", "markdown", "matlab", "muttrc", "neomuttrc", "netrc", "netrw", "nix", "nroff", "perl", "pgn", "plaintex", "rasi", "snippets", "snippets_snipmate", "syntax", "template", "texinfo", "texmf", "tmux", "xdefaults", "zsh"],
	"coc.source.word.shortcut": "D",
	"coc.source.word.priority": 1,
	"suggest.noselect": true,
	"suggest.maxPreviewWidth": 80,
	"suggest.enablePreview": true,
	"suggest.floatEnable": true,
	"suggest.detailField": "preview",
	"suggest.echodocSupport": true,
	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
	"latex.forwardSearch.executable": "zathura",
	"latex.forwardSearch.args": ["%p"],
	"coc.source.bibtex.filetypes": [
		"tex",
		"plaintex",
		"latex",
		"pandoc",
		"markdown"
	],
	"coc.source.bibtex.triggerPatterns": ["\\\\cite\\{"],
	"coc.source.bibtex.triggerCharacters": [],
	"list.source.bibtex.files": ["~/.local/share/texmf/bibtex/bib/matriHX.bib"],
	"list.source.bibtex.citation" : {
		"before": "\\cite{",
		"after": "}"
	},
	"solargraph.trace.server": "messages",
	"solargraph.promptDownload": true,
	"solargraph.hover": true,
	"solargraph.diagnostics": true,
	"solargraph.formatting": true,
	"solargraph.folding": true,
	"solargraph.references": true,
	"solargraph.definitions": true,
	"solargraph.symbols": true,
	"Lua.diagnostics.globals": ["vim", "define", "it"],
	"codeLens.enable": true,
	"languageserver": {
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": ["--lsp"],
			"rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
			"filetypes": ["haskell", "lhaskell"],
			"settings": {
				"haskell": {
					"formattingProvider": "brittany"
				}
			}
		}
	}
}
